{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "migrate": "php artisan migrate --seed",
        "laravel": "php artisan serve",
        "start-dev": "concurrently \"npm run migrate\" \"npm run laravel\"",
        "start-prod": "concurrently \"npm run build\" \"npm run laravel\""
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.0.0",
        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^2.0.0",
        "tailwindcss": "^4.0.0",
        "vite": "^7.0.7"
    }
}
